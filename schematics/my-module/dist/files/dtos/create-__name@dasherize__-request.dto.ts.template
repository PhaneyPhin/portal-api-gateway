import { IsNotEmpty, MaxLength } from 'class-validator';
import { ApiProperty } from '@nestjs/swagger';

export class Create<%= classify(name) %>RequestDto {
  <% fields.forEach((field) => { %>@ApiProperty()
  @IsNotEmpty()
  @MaxLength(160)
  <%= camelize(field) %>: string;

  <% }) %>
  
  createdBy: string;
}
